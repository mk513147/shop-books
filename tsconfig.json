{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"module": "ESNext",
		"baseUrl": ".",
		"paths": {
			"@theme": ["src/theme/index"],
			"@screens/*": ["src/screens/*"],
			"@components/*": ["src/components/*"],
			"@database/*": ["src/database/*"],
			"@navigation/*": ["src/navigation/*"],
			"@constants/*": ["src/constants/*"],
			"@utils/*": ["src/utils/*"],
			"@context/*": ["src/context/*"]
		}
	}
}
